@model Event

@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/AdminE/Views/Shared/_AdminLayout.cshtml";
}



<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Event Table</h4>
                <p class="card-description">
                    <div class="table-responsive">

                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Location</th>
                                    <th>Title</th>
                                    <th>StartTime</th>
                                    <th>EndTime</th>

                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                 <td>@Model.Location</td>
                                    <td>@Model.Title</td>
                                    <td>@Model.StartTime</td> 
                                    <td>@Model.EndTime</td>                                  
                                </tr>
                            </tbody>
                        </table>
                         <a asp-controller="event" asp-action="index" class="btn btn-primary">Back</a>
                    </div>
            </div>
        </div>
    </div>
</div>
